{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "react starter with exposed config",
  "author": "amazingrv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run server",
    "server": "node ./server/server.dev.js",
    "build": "npm run clean && npm run compile:app:prod && npm run prod:deps",
    "prod:deps": "cd ./dist && npm install --omit=dev && cd ..",
    "compile:app:prod": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "core-js": "^3.30.1",
    "ejs": "^3.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "^1.3.56",
    "autoprefixer": "^10.4.14",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.6",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.7",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.2",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.3",
    "webpack-merge": "^5.8.0"
  }
}
