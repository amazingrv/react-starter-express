        "html-webpack-plugin": "^5.1.0",
        "mini-css-extract-plugin": "^1.3.9",
        "moment-locales-webpack-plugin": "^1.2.0",
        "postcss": "^8.2.6",
        "postcss-loader": "^5.0.0",
        "prettier": "^2.2.1",
        "progress-webpack-plugin": "^1.0.12",
