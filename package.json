{
    "name": "react-starter",
    "version": "2.0.0",
    "description": "react starter with exposed config",
    "keywords": [
        "react",
        "redux",
        "webpack",
        "babel 7",
        "starter",
        "config"
    ],
    "author": "amazingrv",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist",
        "start": "npm run clean && npm run server:dev",
        "server:dev": "node ./server/server.dev.js",
        "build": "npm run clean && npm run compile:app:prod && npm run prod:deps",
        "prod:deps": "cd ./dist && npm install --only=prod && cd ..",
        "compile:app:prod": "webpack --mode production --config webpack.prod.js"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.34",
        "@fortawesome/free-solid-svg-icons": "^5.15.2",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "@reduxjs/toolkit": "^1.5.0",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "core-js": "^3.9.1",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "reactstrap": "^8.9.0",
        "redux-logger": "^3.0.6",
        "regenerator-runtime": "^0.13.7"
    },
    "devDependencies": {
        "@babel/core": "^7.13.8",
        "@babel/eslint-parser": "^7.13.8",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-export-default-from": "^7.12.13",
        "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
        "@babel/plugin-proposal-optional-chaining": "^7.13.8",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.13.9",
        "@babel/preset-react": "^7.12.13",
        "autoprefixer": "^10.2.4",
        "babel-loader": "^8.2.2",
        "babel-plugin-lodash": "^3.3.4",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^5.1.1",
        "css-minimizer-webpack-plugin": "^1.2.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^2.5.2",
        "eventsource-polyfill": "^0.9.6",
        "file-loader": "^6.2.0",
