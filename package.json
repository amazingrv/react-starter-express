{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "react starter with exposed config",
  "author": "amazingrv",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run server",
    "server": "node server.dev.js",
    "build": "npm run clean && npm run compile:app:prod && npm run prod:deps",
    "prod:deps": "cd ./dist && npm install --omit=dev && cd ..",
    "compile:app:prod": "webpack --config webpack.prod.js"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "core-js": "^3.45.1",
    "handlebars": "^4.7.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@swc/core": "^1.13.5",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^2.8.8",
    "react-refresh": "^0.17.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
