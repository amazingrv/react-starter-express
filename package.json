        "lodash-webpack-plugin": "^0.11.6",
        "mini-css-extract-plugin": "^1.4.0",
        "moment-locales-webpack-plugin": "^1.2.0",
        "postcss": "^8.2.8",
        "postcss-loader": "^5.2.0",
        "prettier": "^2.2.1",
