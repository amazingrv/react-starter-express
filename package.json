{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "react starter with exposed config",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel 7",
    "starter",
    "config"
  ],
  "author": "amazingrv",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "husky install",
    "cz": "cz",
    "start": "npm run clean && npm run server:dev",
    "server:dev": "node ./server/server.dev.js",
    "build": "npm run clean && npm run compile:app:prod && npm run prod:deps",
    "prod:deps": "cd ./dist && npm install --only=prod && cd ..",
    "compile:app:prod": "webpack --mode production --config webpack.prod.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.24.0",
    "bootstrap": "^4.6.0",
    "core-js": "^3.20.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "history": "^5.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.0.2",
    "reactstrap": "^8.10.0",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/cz-commitlint": "^15.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^2.6.0",
    "eventsource-polyfill": "^0.9.6",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.4",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.4.5",
    "moment-locales-webpack-plugin": "^1.2.0",
    "postcss": "^8.4.0",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.0",
    "webpack-dev-middleware": "^5.2.2",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
