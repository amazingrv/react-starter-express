{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "react starter with exposed config",
  "author": "amazingrv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run server",
    "server": "node ./server/server.dev.js",
    "build": "npm run clean && npm run compile:app:prod && npm run prod:deps",
    "prod:deps": "cd ./dist && npm install --omit=dev && cd ..",
    "compile:app:prod": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "core-js": "^3.27.2",
    "ejs": "^3.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "^1.3.32",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^9.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.6",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.4",
    "react-refresh": "^0.14.0",
    "rimraf": "^4.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  }
